{
  "name": "meme-as-a-service",
  "version": "1.0.6",
  "description": "A node library + CLI that generates memes dynamically for Slack/Discord bots using templates",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "meme-as-a-service": "dist/cli.js"
  },
  "files": [
    "dist/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "cli": "node dist/cli.js",
    "api": "node dist/api/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "setup": "node scripts/download-templates.js",
    "setup-real": "node scripts/download-real-templates.js",
    "add-templates": "node scripts/add-more-templates.js",
    "template-wizard": "node scripts/template-wizard.js",
    "coordinate-helper": "node scripts/coordinate-helper.js",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "meme",
    "generator",
    "slack",
    "discord",
    "bot",
    "cli",
    "api",
    "templates"
  ],
  "author": "Ankit Sharma <ankit.sharma199803@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "sharp": "^0.32.6",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.0",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/multer": "^1.4.9",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.7",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beingmartinbmc/meme-as-a-service.git"
  },
  "bugs": {
    "url": "https://github.com/beingmartinbmc/meme-as-a-service/issues"
  },
  "homepage": "https://github.com/beingmartinbmc/meme-as-a-service#readme"
}
